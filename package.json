{
  "name": "webpack-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env mode=development",
    "build:dev": "tsc --noEmit ./src/index.tsx && webpack --env mode=development",
    "build:prod": "tsc --noEmit ./src/index.tsx && webpack --env mode=production",
    "lint": "biome lint --fix ./src",
    "format": "biome format --fix ./src",
    "ci": "tsc --noEmit ./src/index.tsx && biome ci --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true --assists-enabled=true ./src",
    "typecheck": "tsc"
  },
  "keywords": [],
