    "@biomejs/biome": "1.9.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "babel-loader": "^9.2.1",
